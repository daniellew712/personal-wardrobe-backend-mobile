{
  "name": "personal-wardrobe-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "node build.js",
    "start": "node ./dist/bin/www.js",
    "dev": "nodemon --exec ts-node ./src/bin/www.ts",
    "watch": "tsc --watch",
    "seed": "ts-node ./prisma/seed.ts",
    "deploy": "prisma db push --accept-data-loss && node ./dist/bin/www.js"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.1",
    "express": "~4.16.1",
    "firebase-admin": "^13.4.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "prisma": "^6.12.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.15",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
